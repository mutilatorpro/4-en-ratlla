            <VBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="35.0" fitWidth="220.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/LOGO1.png" />
                     </image>
                     <VBox.margin>
                        <Insets bottom="7.0" top="25.0" />
                     </VBox.margin>
                  </ImageView>
                  <HBox alignment="CENTER" prefHeight="115.0" prefWidth="484.0" spacing="40.0">
                     <children>
                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="15.0">
                           <children>
                                <Button fx:id="botAutenticar" onAction="#finestraAutenticar" text="Autenticar-se" />
